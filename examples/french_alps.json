{
  "top_layer_block": "minecraft:grass_block",
  "top_layer_thickness": 6,
  "bottom_layer_block": "minecraft:stone",
  "base_biome": "minecraft:plains",
  "generate_features": true,
  "cliff_generation": {
    "enabled": true,
    "angle_threshold_degrees": 55.0,
    "block": "minecraft:stone",
    "smoothing_radius": 2,
    "smoothing_factor": 0.35
  },
  "biome_layers": [
    { "range": { "min": "3300m" },                 "biome": "minecraft:frozen_peaks" },
    { "range": { "min": "2500m", "max": "3300m" }, "biome": "minecraft:snowy_slopes" },
    { "range": { "min": "1800m", "max": "2500m" }, "biome": "minecraft:stony_peaks" },
    { "range": { "min": "1200m", "max": "1800m" }, "biome": "minecraft:windswept_hills" },
    { "range": { "min": "800m",  "max": "1200m" }, "biome": "minecraft:taiga" },
    { "range": { "min": "200m",  "max": "800m" },  "biome": "minecraft:forest" },
    { "range": { "min": "50m",   "max": "200m" },  "biome": "minecraft:birch_forest" },
    { "range": { "max": "5m" },                    "biome": "minecraft:beach" }
  ],
  "top_block_layers": [
    { "range": { "min": "3300m" },                 "block": "minecraft:packed_ice" },
    { "range": { "min": "2500m", "max": "3300m" }, "block": "minecraft:snow_block" },
    { "range": { "min": "1800m", "max": "2500m" }, "block": "minecraft:stone" },
    { "range": { "max": "8m" },                    "block": "minecraft:sand" }
  ],
  "osm": {
    "enabled": true,
    "bbox_margin_m": 500.0,
    "layers": [
      {
        "name": "trails_and_tracks",
        "geometry": "line",
        "width_m": 1.0,
        "query": "(way[\"highway\"~\"^(track|path)$\"]({{bbox}}););",
        "style": {
          "surface_block": "minecraft:coarse_dirt",
          "top_thickness": 1
        }
      },
      {
        "name": "paved_roads",
        "geometry": "line",
        "width_m": 5.0,
        "query": "(way[\"highway\"~\"^(motorway|trunk|primary|secondary|tertiary|residential)$\"]({{bbox}}););",
        "style": {
          "surface_block": "minecraft:black_concrete",
          "top_thickness": 1
        }
      },
      {
        "name": "waterways",
        "geometry": "line",
        "width_m": 4.0,
        "query": "(way[\"waterway\"~\"^(river|stream)$\"]({{bbox}}););",
        "style": {
          "surface_block": "minecraft:water",
          "top_thickness": 4,
          "subsurface_block": "minecraft:dirt"
        }
      },
      {
        "name": "water_polygons",
        "geometry": "polygon",
        "query": "(way[\"natural\"=\"water\"]({{bbox}});relation[\"natural\"=\"water\"]({{bbox}}););",
        "style": {
          "surface_block": "minecraft:water",
          "top_thickness": 5,
          "subsurface_block": "minecraft:clay"
        }
      },
      {
        "name": "dense_forest",
        "geometry": "polygon",
        "query": "(way[\"natural\"=\"wood\"]({{bbox}});way[\"landuse\"=\"forest\"]({{bbox}});relation[\"landuse\"=\"forest\"]({{bbox}}););",
        "style": {
          "biome": "minecraft:forest",
          "surface_block": "minecraft:grass_block"
        }
      },
      {
        "name": "subalpine_meadows",
        "geometry": "polygon",
        "query": "(way[\"natural\"~\"^(grassland|heath)$\"]({{bbox}});way[\"landuse\"~\"^(meadow|farmland)$\"]({{bbox}}););",
        "style": {
          "biome": "minecraft:meadow",
          "surface_block": "minecraft:grass_block"
        }
      },
      {
        "name": "buildings",
        "geometry": "polygon",
        "query": "(way[\"building\"]({{bbox}});relation[\"building\"]({{bbox}}););",
        "style": {
          "surface_block": "minecraft:black_concrete",
          "top_thickness": 4,
          "subsurface_block": "minecraft:stone"
        }
      }
    ]
  }
}
